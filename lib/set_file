execute_in_container_shebang(){
    docker exec \
      -u $(getvar cosh_user) \
      -i `get_target_container` \
      /bin/sh < `getvar execute_lines`
}

set_file(){
  sanitize "$*";
  test -f "$1" || return 1
  setvar execute_lines $1

  execute_in_container(){ execute_in_container_shebang; }
}
